

@{
    ViewData["Title"] = "Home Page";
    var UsersData = ViewBag.quotes1;
    //asp-all-route-data
}
<header>
    <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
        <div class="container-fluid">
            <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">CRUD App</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Quotes</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="getAccount">Accounts</a>
                        </li>
                </ul>
            </div>
        </div>
    </nav>
</header>
<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <div><b>Quotes</b><br /></div>
    <table class="table table-bordered table-responsive table-hover">
        <tr>
            <th>Quote Name </th>
            <th>Proposal Stage</th>
            <th>Proposal Net Amount</th>
        </tr>
        @if (ViewBag.quotes1 != null)
        {
            @foreach (var d in (ViewBag.quotes1))
            {
                <tr>
                    <td><a asp-action="singleQuote" asp-controller="Home" asp-route-Id=@d.Id>@d.Name</a></td>
                    <td>@d.Apttus_Proposal__Approval_Stage__c</td>
                    <td>@d.Apttus_Proposal__Net_Amount__c</td>
                </tr>
            }
        }
    </table>
    <input type="button" value="Previous" onclick="PreviosPage()"/>
    @TempData["PageNumber"]
    <input type="button" value="Next" onclick="NextPage()" />

    <script>
        function PreviosPage(){
            var pageNum = '@TempData["PageNumber"]';
            console.log("PageNumber::"+pageNum);
        }
    </script>

    
    <form asp-action="logout" asp-controller="Login" method="post">
        <input type="submit" id="submit" name="submit" value="Logout" />
    </form>
</div>
